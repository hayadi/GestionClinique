<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Medecin.
    -->
    <changeSet id="20191009114028-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="specialite_id"
                                 baseTableName="medecin"
                                 constraintName="fk_medecin_specialite_id"
                                 referencedColumnNames="id"
                                 referencedTableName="specialite"/>

        <addForeignKeyConstraint baseColumnNames="clinique_id"
                                 baseTableName="medecin"
                                 constraintName="fk_medecin_clinique_id"
                                 referencedColumnNames="id"
                                 referencedTableName="clinique"/>

    </changeSet>
</databaseChangeLog>
